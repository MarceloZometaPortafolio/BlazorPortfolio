@page "/resume"
@inject IJSRuntime JSRuntime
<h3>Resume</h3>

<div id="viewer" style='width: 1024px; height: 600px; margin: 0 auto;'></div>

<img src="images/resume.png" class="justify-content-lg-center" asp-append-version="true" alt="Marcelo Zometa's Resume"/>

@code {
    protected override async void OnAfterRender(bool render)
    {
        await JSRuntime.InvokeAsync<object>("webviewerFunctions.initWebViewer");
    }
    
}
